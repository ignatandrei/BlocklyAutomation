{
  "name": "ang-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200 --open",
    "build": "ng version && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "buildDesktop": "ng build --base-href .",
    "buildGitHubPages": "ng build --base-href /BlocklyAutomation/",
    "buildLibrary":"ng build Node2Blockly",
    "addPWA": "ng add @angular/pwa  --defaults true --skip-confirmation true",
    "pwaRun": "ng build --output-path pwa"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/cdk": "^13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/material": "^13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@blockly/workspace-content-highlight": "1.0.10",
    "@ctrl/ngx-codemirror": "5.1.1",
    "@types/intro.js": "^3.0.2",
    "blockly": "6.20210701.0",
    "chart.js": "^3.6.2",
    "codemirror": "5.65.0",
    "intro.js": "^4.3.0",
    "rxjs": "~7.4.0",
    "tabulator-tables": "5.0.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.2",
    "@angular/cli": "^13.1.2",
    "@angular/compiler-cli": "~13.1.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@types/tabulator-tables": "5.0.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.0.0",
    "typescript": "~4.5.2"
  }
